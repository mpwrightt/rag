{
  "version": 2,
  "installCommand": "pnpm install --no-frozen-lockfile",
  "buildCommand": "pnpm run build",
  "routes": [
    {
      "src": "/api/rag/(.*)",
      "dest": "/api/vercel_handler.py"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "PYTHONPATH": "."
  },
  "regions": ["iad1"]
}
